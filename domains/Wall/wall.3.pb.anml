action line1(Brick b0 , Brick sb0, Brick sb1){
	constant Worker w0;
	constant Worker w1;
	constant Worker w2;
	constant Truck t0;
	[all] contains {
		pi0 : pick(w0, b0, t0);
		g0 : goto(w0, t0,  wall);
		pl0 : place(w0, b0, sb0, sb1);
		 c0 : putCementBrick(w1, sb0);
		 c1 : putCementBrick(w2, sb1);
	};
	end(pi0) < start(g0);
	end(g0) < start(pl0);
	end(c0) < start(pl0);
	end(c1) < start(pl0);
	end(pl0) < end(c0) + 10;
	end(pl0) < end(c1) + 10;
};
action line2(Brick b0 , Brick b1, Brick sb0, Brick sb1, Brick sb2){
	constant Worker w0;
	constant Worker w1;
	constant Worker w2;
	constant Worker w3;
	constant Worker w4;
	constant Truck t0;
