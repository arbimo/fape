	[all] contains {
		pi0 : pick(w0, b0, t0);
		g0 : goto(w0, t0,  wall);
		pl0 : place(w0, b0, sb0, sb1);
		pi1 : pick(w1, b1, t1);
		g1 : goto(w1, t1,  wall);
		pl1 : place(w1, b1, sb1, sb2);
		 c0 : putCementBrick(w2, sb0);
		 c1 : putCementBrick(w3, sb1);
		 c2 : putCementBrick(w4, sb2);
	};
	end(pi0) < start(g0);
	end(g0) < start(pl0);
	end(c0) < start(pl0);
	end(c1) < start(pl0);
	end(pl0) < end(c0) + 10;
	end(pl0) < end(c1) + 10;
	end(pi1) < start(g1);
	end(g1) < start(pl1);
	end(c1) < start(pl1);
	end(c2) < start(pl1);
	end(pl1) < end(c1) + 10;
	end(pl1) < end(c2) + 10;
};
action line3(Brick b0 , Brick b1, Brick b2, Brick sb0, Brick sb1, Brick sb2, Brick sb3){
	constant Worker w0;
	constant Worker w1;
	constant Worker w2;
	constant Worker w3;
	constant Worker w4;
	constant Worker w5;
	constant Worker w6;
	constant Truck t0;
	constant Truck t1;
	constant Truck t2;
	[all] contains {
		pi0 : pick(w0, b0, t0);
		g0 : goto(w0, t0,  wall);
		pl0 : place(w0, b0, sb0, sb1);
		pi1 : pick(w1, b1, t1);
		g1 : goto(w1, t1,  wall);
		pl1 : place(w1, b1, sb1, sb2);
		pi2 : pick(w2, b2, t2);
		g2 : goto(w2, t2,  wall);
		pl2 : place(w2, b2, sb2, sb3);
		 c0 : putCementBrick(w3, sb0);
		 c1 : putCementBrick(w4, sb1);
		 c2 : putCementBrick(w5, sb2);
		 c3 : putCementBrick(w6, sb3);
	};
	end(pi0) < start(g0);
	end(g0) < start(pl0);
	end(c0) < start(pl0);
	end(c1) < start(pl0);
	end(pl0) < end(c0) + 10;
	end(pl0) < end(c1) + 10;
	end(pi1) < start(g1);
	end(g1) < start(pl1);
	end(c1) < start(pl1);
	end(c2) < start(pl1);
	end(pl1) < end(c1) + 10;
	end(pl1) < end(c2) + 10;
	end(pi2) < start(g2);
	end(g2) < start(pl2);
	end(c2) < start(pl2);
	end(c3) < start(pl2);
	end(pl2) < end(c2) + 10;
	end(pl2) < end(c3) + 10;
};
action build3(Brick b0, Brick b1, Brick b2, Brick b3, Brick b4, Brick b5){
	b0 != b1;
	b0 != b2;
	b0 != b3;
	b0 != b4;
	b0 != b5;
	b1 != b2;
	b1 != b3;
	b1 != b4;
	b1 != b5;
	b2 != b3;
	b2 != b4;
	b2 != b5;
	b3 != b4;
	b3 != b5;
	b4 != b5;
	[start] {
		b0.loc == t0;
		b1.loc == t0;
		b2.loc == t0;
		b3.loc == t0;
		b4.loc == t0;
		b5.loc == t0;
	};
	[all] contains {
		l1 : line1(b0, b1, b2);
		l2 : line2(b1, b2, b3, b4, b5);
		l3 : line3( b3, b4, b5, none0, none1, none2, none3);
	};
	end(l3) < start(l2);
	end(l2) < start(l1);
};
instance Brick b0, b1, b2, b3, b4, b5;
instance Worker w0, w1, w2;
instance Brick none0, none1, none2, none3;
instance Truck t0, t1;
[start] {
	none0.loc := wall;
	none0.cemented := false;	
	none1.loc := wall;
	none1.cemented := false;	
	none2.loc := wall;
	none2.cemented := false;	
	none3.loc := wall;
	none3.cemented := false;	
	haveCement := false;
	b0.cemented := false; 
	b0.loc := t0; 
	b1.cemented := false; 
	b1.loc := t0; 
	b2.cemented := false; 
	b2.loc := t0; 
	b3.cemented := false; 
	b3.loc := t0; 
	b4.cemented := false; 
	b4.loc := t0; 
	b5.cemented := false; 
	b5.loc := t0; 
	w0.at := t0;
	w0.available := true;
	w1.at := t0;
	w1.available := true;
	w2.at := t0;
	w2.available := true;
};
[all]contains {
	build3(b0, b1, b2, b3, b4, b5);
};
