instance Block A, C, D, B;
[start] clear(B) := true;
[start] on(D) := TABLE;
[start] on(B) := TABLE;
[start] clear(C) := true;
[start] on(C) := TABLE;
[start] clear(A) := true;
[start] on(A) := TABLE;
[start] clear(D) := true;
[start] handempty := true;
[end] on(D) == C;
[all] contains DoStack(D, C);
[end] on(C) == A;
[all] contains DoStack(C, A);
[end] on(A) == B;
[all] contains DoStack(A, B);
